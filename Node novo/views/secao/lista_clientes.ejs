      </li>

      <li>
        <a href="/lista_clientes">Lista de Clientes</a>
      </li>
    </nav>
    <main>
      <article>
        <header>
          <h1 id="titulo">Lista de Clientes</h1>
        </header>
        <div id="tabela">
          <div class="celula_titulo">Nome</div>
          <div class="celula_titulo">CPF</div>
          <div class="celula_titulo">Celular</div>
          <% clientes.forEach(regClientes => { %>
            <div class="celula" >
              <%= regClientes.dataValues.nome %>
            </div>
            <div class="celula">
              <%= regClientes.dataValues.cpf %>
            </div>
            <div class="celula">
              <%= regClientes.dataValues.celular %>
            </div>
          </tr>
          <% }) %>
        </div>
      </article>
    </main>
  </body>
</html>
